AD_CLASS_BEGIN

AD_CONSTRUCTOR5([in],aafAuid_t,Auid,auid for this definition object,
				[in],aafString_t, Name, Unique name of this Data definition,
				[in],aafVersionType_t, minRev, Minimum AAF Version for this type,
				[in],aafVersionType_t, maxRev, Maximum AAF Version for this type,
				[in],aafSwapNeeded_t, swapInfo, Specifies wheter the bytes in this type need to be swapped,
				Constructs and sets the AUID ad Name of this TypeDefinition object.)

AD_METHOD1(GetName,
		   [retval][out],
		   aafString_t *,
		   pstrName,
		   Pointer to name,
		   Gets the unique name that identifies this datakind object.)

AD_METHOD1(GetMinVersion,
		   [retval][out],
		   aafVersion_t *,
		   pMinVersion,
		   Pointer to the minimum version ,
		   Gets the Minimum AAF Version for this type object.)

AD_METHOD1(SetMinVersion,
		   [in],
		   aafVersion_t,
		   MinVersion,
		   Minimum AAF Version,
		   Sets the Minimum AAF Version for type definition.)

AD_METHOD1(GetMaxVersion,
		   [retval][out],
		   aafVersion_t *,
		   pMaxVersion,
		   Pointer to the maximum version ,
		   Gets the Maximum AAF Version for this type object.)

AD_METHOD1(SetMaxVersion,
		   [in],
		   aafVersion_t,
		   MaxVersion,
		   Maximum AAF Version,
		   Sets the Maximum AAF Version for type definition.)

AD_METHOD1(GetSwapNeeded,
		   [retval][out],
		   aafSwapNeeded_t,
