open( PROCESS, "StripEscapedReturns |" ) or die "Failed to run StripEscapedReturns";

while( <PROCESS> ) {

next if ( /^\/usr\/freeware\/bin\/gmake / );
next if ( /^gmake/ );
next if ( /^cd / );
next if ( /^Building/ );
next if ( /^Generating/ );
next if ( /^CC / );
next if ( /^ CC / );
next if ( /^cd / );
next if ( /^cp / );
next if ( /\-\-\- C\+\+ prelinker/ );

print;

}

close(PROCESS);
