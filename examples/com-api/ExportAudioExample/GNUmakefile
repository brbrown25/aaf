EXAMPLE = ExportAudioExample

include ../comexamplerules.mk

.PHONY : check
check : $(AAFSDKBINDIR)/ExportAudioExample$(EXE)
	@if [ ! -r Laser.wav ]; then cp ../../../test/com/ComModTestAAF/Laser.wav .; fi
	$(APP_RUN_ENV) $(AAFSDKBINDIR)/ExportAudioExample$(EXE)
